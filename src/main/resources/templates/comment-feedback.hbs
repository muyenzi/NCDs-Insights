
{{#partial "content"}}


    <div class="alert alert-dark" role="alert" style="font-family: 'Cormorant Garamond', serif;">
        Thanks for your feedback!
    </div>
    <div class="container">

        <div class="jumbotron" style="width:50%">
            <h1 class="display-4"></h1>
            {{#if allComments}}
                {{#each allComments}}
                    <h4 style="font-family: 'Cormorant Garamond', serif;color:#5D0084"><strong>{{name}}</strong></h4>

                    <pre style="font-family: 'Cormorant Garamond', serif;" class="lead">{{message}}</pre>

                    <hr class="my-4">
                {{/each}}
            {{else}}
            {{/if}}
            <p></p>
            <a class="btn btn-primary btn-lg" href="comments/new" role="button">Give your FeedBack</a>
        </div>
    </div>
{{/partial}}
{{> layout.hbs}}